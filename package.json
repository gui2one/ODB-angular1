{
  "name": "my-app",
  "version": "0.0.0",
  "license": "MIT",
  "config": {
    "deploy_path": "/test/path/"
  },
  "scripts": {
    "ng": "ng",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ngc": "ngc -p tsconfig.json && cp app/* compiled",
    "build": "ng build --prod",

    "gui2one_sync": ".\\gui2one_sync.bat",
    "gui2one_sync-ODB-cyberdev": ".\\gui2one_sync_ODB_cyberdev.bat",

    "build-dev": "ng build && npm run gui2one_sync",
    "build-test-dev": "ng build --watch --base-href /preview/ --deploy-url /preview/ --output-path ./dist/preview",
    "build-test-prod": "ng build --prod --aot --build-optimizer --base-href /preview/ --deploy-url /preview/ --output-path ./dist/preview",
    "build-no-test-dev": "ng build --watch --base-href / --deploy-url / --output-path ./dist",
    "prestart": "ng build --prod  && ngc",
    "start": "ts-node src/server.ts"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.2",
    "@angular/animations": "^5.2.9",
    "@angular/common": "^5.2.9",
    "@angular/compiler": "^5.2.9",
    "@angular/core": "^5.2.9",
    "@angular/forms": "^5.2.9",
    "@angular/http": "^5.2.9",
    "@angular/platform-browser": "^5.2.9",
    "@angular/platform-browser-dynamic": "^5.2.9",
    "@angular/platform-server": "^5.2.9",
    "@angular/router": "^5.2.9",
    "angularfire2": "^5.0.0-rc.5-next",
    "bootstrap": "4.0.0-beta.2",
    "classlist.js": "^1.1.20150312",
    "compass-mixins": "^0.12.10",
    "core-js": "^2.4.1",
    "firebase": "4.8.0",
    "jquery": "^3.3.1",
    "jquery-ui": "^1.12.1",
    "ng-lazyload-image": "^3.4.2",
    "ng-static-site-generator": "^0.10.1",
    "ng2-carouselamos": "^3.2.0",
    "ngx-auth": "^2.3.0",
    "ngx-bootstrap": "^2.0.3",
    "ngx-ckeditor": "^0.3.1",
    "npm-run-batch": "0.0.6",
    "popper.js": "^1.14.1",
